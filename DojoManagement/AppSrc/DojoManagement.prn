98345>>>>>>>>>>>    // internal
98345>>>>>>>>>>>    // parse passed format string and set temporary properties with result
98345>>>>>>>>>>>    //
98345>>>>>>>>>>>    Procedure ParseFormat string sFmt
98347>>>>>>>>>>>
98347>>>>>>>>>>>        string sLeft sRight sDigit
98347>>>>>>>>>>>        integer bSep iPos i iDigits
98347>>>>>>>>>>>
98347>>>>>>>>>>>        // replace any literals. A "/" followed by anything.
98347>>>>>>>>>>>        // some literals are special. $ . , / #
98347>>>>>>>>>>>        Move (Replaces("/$",sFmt,Character(1)))    to sFmt
98348>>>>>>>>>>>        Move (Replaces("/.",sFmt,Character(2)))    to sFmt
98349>>>>>>>>>>>        Move (Replaces("/,",sFmt,Character(3)))    to sFmt
98350>>>>>>>>>>>        Move (Replaces("/"+"/",sFmt,Character(4))) to sFmt
98351>>>>>>>>>>>        Move (Replaces("/#",sFmt,Character(5)))    to sFmt
98352>>>>>>>>>>>        Move (Character(9)) to sDigit
98353>>>>>>>>>>>        Move (Replaces("#",sFmt,sDigit))           to sFmt
98354>>>>>>>>>>>        Move (Replaces("/",sFmt,""))               to sFmt // replace all others
98355>>>>>>>>>>>
98355>>>>>>>>>>>        Move (Pos(",",sFmt))                 to bSep // if we have any , we use thousand seps
98356>>>>>>>>>>>        If bSep Move (Replaces(",",sFmt,"")) to sFmt // remove all ,
98359>>>>>>>>>>>
98359>>>>>>>>>>>        Move (Replaces("$",sFmt,psCurrencySymbol(self))) to sFmt // replace any $ with currency symbol
98360>>>>>>>>>>>
98360>>>>>>>>>>>        Move (Pos(".",sFmt)) to iPos                 // position of decimal
98361>>>>>>>>>>>
98361>>>>>>>>>>>        // Move all the special literals back into place before parsing
98361>>>>>>>>>>>        Move (Replaces(Character(1),sFmt,"$")) to sFmt
98362>>>>>>>>>>>        Move (Replaces(Character(2),sFmt,".")) to sFmt
98363>>>>>>>>>>>        Move (Replaces(Character(3),sFmt,",")) to sFmt
98364>>>>>>>>>>>        Move (Replaces(Character(4),sFmt,"/")) to sFmt
98365>>>>>>>>>>>        Move (Replaces(Character(5),sFmt,"#")) to sFmt
98366>>>>>>>>>>>
98366>>>>>>>>>>>        If (iPos>0) Begin                      // if we have a decimanl point
98368>>>>>>>>>>>            Move 1 to i                        // look for first non # to right and count the #s
98369>>>>>>>>>>>            If (mid(sFmt,1,iPos+i)="*") Begin  // the "*" is special. It means as many as you want
98371>>>>>>>>>>>               Move -2 to iDigits
98372>>>>>>>>>>>               increment i
98373>>>>>>>>>>>            end
98373>>>>>>>>>>>>
98373>>>>>>>>>>>            While (mid(sFmt,1,iPos+i)=sDigit)
98377>>>>>>>>>>>                 Increment i
98378>>>>>>>>>>>            end
98379>>>>>>>>>>>>
98379>>>>>>>>>>>            Move (Mid(sFmt,255,iPos+i)) to sRight // everything to the right of the last # is format stuff
98380>>>>>>>>>>>            If (iDigits=0) Move (i-1) to iDigits
98383>>>>>>>>>>>            //
98383>>>>>>>>>>>            Move 1 to i                           // find the first non-# to the left of the point
98384>>>>>>>>>>>            While (mid(sFmt,1,iPos-i)=sDigit)     // everything to the left is format stuff
98388>>>>>>>>>>>                 increment i
98389>>>>>>>>>>>            end
98390>>>>>>>>>>>>
98390>>>>>>>>>>>            Move (left(sFmt,iPos-i)) to sLeft
98391>>>>>>>>>>>        end
98391>>>>>>>>>>>>
98391>>>>>>>>>>>        else begin                             // we have no decinal
98392>>>>>>>>>>>            Move 0 to iDigits                  // so points is none
98393>>>>>>>>>>>            Move (Pos(sDigit,sFmt)) to iPos    // find first #.
98394>>>>>>>>>>>            If (iPos=0) Begin                  // if none, entire string is left format stuff..wierd!
98396>>>>>>>>>>>                Move sFmt to sLeft
98397>>>>>>>>>>>                Move ""   to sRight
98398>>>>>>>>>>>            end
98398>>>>>>>>>>>>
98398>>>>>>>>>>>            else begin
98399>>>>>>>>>>>                Move (left(sFmt,iPos-1)) to sLeft // all char to left of first # is left format stuff
98400>>>>>>>>>>>                Move 1 to i
98401>>>>>>>>>>>                While (mid(sFmt,1,iPos+i)=sDigit) // find last #, all char to right is right format
98405>>>>>>>>>>>                    Increment i
98406>>>>>>>>>>>                end
98407>>>>>>>>>>>>
98407>>>>>>>>>>>                Move (Mid(sFmt,255,i+iPos)) to sRight
98408>>>>>>>>>>>            end
98408>>>>>>>>>>>>
98408>>>>>>>>>>>        end
98408>>>>>>>>>>>>
98408>>>>>>>>>>>        // set temporary format properties and exit
98408>>>>>>>>>>>        Set pbThousandsSep to bSep
98409>>>>>>>>>>>        set psLeft         to sLeft
98410>>>>>>>>>>>        set psRight        to sRight
98411>>>>>>>>>>>        Set piPoints       to iDigits
98412>>>>>>>>>>>     End_procedure
