101581>>>>>>>    Function Ensure_Visible String sTag Returns Boolean
101583>>>>>>>        Boolean bFound bGroupRow bExpanded
101583>>>>>>>        Integer iCount iLoop
101583>>>>>>>        Handle hoRecords hoRec hoReport_Rows hoRow hoGroupRow
101583>>>>>>>        Variant vRec vRow vReport_Rows vGroup_Row
101583>>>>>>>        String sRecTag
101583>>>>>>>        Integer[] aiRow
101584>>>>>>>                       
101584>>>>>>>        Get phoReportRecords  to hoRecords
101585>>>>>>>        Get phoReportRecord   to hoRec
101586>>>>>>>        Get phoReportRows     to hoReport_Rows
101587>>>>>>>        Get phoReportRow      to hoRow
101588>>>>>>>        Get phoReportGroupRow to hoGroupRow
101589>>>>>>>
101589>>>>>>>        Get ComRows to vReport_Rows
101590>>>>>>>        Set pvComObject of hoReport_Rows to vReport_Rows
101591>>>>>>>
101591>>>>>>>        Get ComCount of hoRecords to iCount
101592>>>>>>>
101592>>>>>>>        //Find record with matching TAG
101592>>>>>>>        Decrement iCount
101593>>>>>>>        Move False to bFound
101594>>>>>>>        Move 0 to iLoop         
101595>>>>>>>        Repeat
101595>>>>>>>>
101595>>>>>>>            Get ComRecord of hoRecords iLoop to vRec
101596>>>>>>>            Set pvComObject of hoRec to vRec
101597>>>>>>>
101597>>>>>>>            Get ComTag of hoRec to sRecTag
101598>>>>>>>            If (sRecTag = sTag) Begin
101600>>>>>>>                Move True to bFound
101601>>>>>>>            End
101601>>>>>>>>
101601>>>>>>>            Increment iLoop
101602>>>>>>>        Until ((iLoop = iCount) or bFound)
101604>>>>>>>
101604>>>>>>>        If (bFound) Begin
101606>>>>>>>            //tag found so find row
101606>>>>>>>            Get ComFindRow of hoReport_Rows vRec to vRow
101607>>>>>>>            If (IsNullComObject(vRow)) Begin
101609>>>>>>>                //Row not found so must be in a collapsed group.
101609>>>>>>>                //Have to expand each group until we find row
101609>>>>>>>                Move False to bFound
101610>>>>>>>                Get ComCount of hoReport_Rows to iCount
101611>>>>>>>                Decrement iCount
101612>>>>>>>                For iLoop from 0 to iCount
101618>>>>>>>>
101618>>>>>>>                    Get ComRow of hoReport_Rows iLoop to vRow
101619>>>>>>>                    Set pvComObject of hoRow to vRow
101620>>>>>>>
101620>>>>>>>                    Get ComGroupRow of hoRow to bGroupRow
101621>>>>>>>                    If (bGroupRow) Begin
101623>>>>>>>                        Set pvComObject of hoGroupRow to vRow
101624>>>>>>>                        Get ComExpanded of hoGroupRow to bExpanded
101625>>>>>>>                        If (not(bExpanded)) Begin
101627>>>>>>>                            Set ComExpanded of hoGroupRow to True
101628>>>>>>>                            Get ComFindRow of hoReport_Rows vRec to vRow
101629>>>>>>>                            If (IsNullComObject(vRow)) Begin
101631>>>>>>>                                Move iLoop to aiRow[SizeOfArray(aiRow)]
101632>>>>>>>                                Get ComCount of hoReport_Rows to iCount
101633>>>>>>>                                Decrement iCount
101634>>>>>>>                            End
101634>>>>>>>>
101634>>>>>>>                            Else Begin
101635>>>>>>>                                Move True to bFound
101636>>>>>>>                                Move (iCount + 1) to iLoop  //bomb loop
101637>>>>>>>                            End
101637>>>>>>>>
101637>>>>>>>                        End
101637>>>>>>>>
101637>>>>>>>                    End            
101637>>>>>>>>
101637>>>>>>>                Loop
101638>>>>>>>>
101638>>>>>>>
101638>>>>>>>                //Collapse any groups that we have just expanded
101638>>>>>>>                Move (SizeOfArray(aiRow)) to iCount
101639>>>>>>>                While (iCount > 0) 
101643>>>>>>>                    Decrement iCount
101644>>>>>>>                    Get ComRow of hoReport_Rows aiRow[iCount] to vGroup_Row
101645>>>>>>>                    Set pvComObject of hoGroupRow to vGroup_Row
101646>>>>>>>                    Set ComExpanded of hoGroupRow to False
101647>>>>>>>                Loop
101648>>>>>>>>
101648>>>>>>>            End
101648>>>>>>>>
101648>>>>>>>        End
101648>>>>>>>>
101648>>>>>>>
101648>>>>>>>        If (bFound) Begin
101650>>>>>>>            //Found row so make it the focus.
101650>>>>>>>            //We use FocusedRow as this makes the row visible and 
101650>>>>>>>            //highlights it. EnsureVisible only makes row visible.
101650>>>>>>>            Set pvComObject of hoRow to vRow
101651>>>>>>>            Set ComFocusedRow to vRow 
101652>>>>>>>            //Send ComEnsureVisible of hoRow
101652>>>>>>>        End
101652>>>>>>>>
101652>>>>>>>
101652>>>>>>>        Function_Return bFound
101653>>>>>>>    End_Function
